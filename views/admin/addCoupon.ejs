<div class="container">
    <h2 class="text-center">COUPONS</h2>
    <form style="padding: 60px;" action="/admin/addCoupon" method="post" enctype="multipart/form-data">
        
        <div class="form-group">
            <label for="name">coupon code</label>
            <input type="name" class="form-control" id="exampleFormControlInput1" name="name">
        </div>

       
        <div class="form-group">
            <label for="name">Expiry Date</label>
            <input type="date" class="form-control" id="exampleFormControlInput1" name="exDate">
        </div>


        <div class="form-group">
            <label for="name">Minimum value to apply Coupon</label>
            <input type="number" value="" class="form-control" id="exampleFormControlInput1" name="minValue">
        </div>

        <div class="form-group">
            <label for="name">Maximum Offer</label>
            <input type="number" value="" class="form-control" id="exampleFormControlInput1" name="maxValue">
        </div>

        <div class="form-group">
            <label for="name">Discount percentage</label>
            <input type="number" class="form-control" id="exampleFormControlInput1" name="Discount">
        </div>



        
        <button class="btn btn-success" type="submit">submit</button>
    </form>


</div>